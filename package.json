{
  "name": "blogs-api-challenge",
  "description": "Repositório de desafio da aplicação blogs-api",
  "version": "0.1.0",
  "scripts": {
    "prestart": "npm run db:drop",
    "start": "ts-node-dev src/index.ts",
    "debug": "ts-node-dev src/index.ts",
    "test": "vitest",
    "db:drop": "npx prisma migrate reset --force --skip-generate",
    "db:seed": "ts-node-dev --transpile-only prisma/seeds/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "2.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/frisby": "^2.0.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.23",
    "@types/shelljs": "^0.8.11",
    "frisby": "2.1.2",
    "prisma": "^4.4.0",
    "shelljs": "^0.8.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4"
  }
}

{
  "name": "blogs-api-challenge",
  "description": "Repositório de desafio da aplicação blogs-api",
  "version": "0.1.0",
  "scripts": {
    "prestart": "npm run db:migrate",
    "start": "npm run debug",
    "debug": "ts-node-dev --transpile-only --ignore-watch node_modules/ src/index.ts",
